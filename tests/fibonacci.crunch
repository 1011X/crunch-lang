fn main()
    println(fibonacci(1))
    println(fibonacci(10))
    println(fibonacci(20))
end

fn fibonacci(n: int) -> int
    if n < 2
        return 1
    else
        return fibonacci(n - 1) + fibonacci(n - 2)
    end
end

