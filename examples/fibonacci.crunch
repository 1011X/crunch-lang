:: args: run --quiet --color=none
:: expected exit status: 55

fn main() -> i64
    return fibonacci(10)
end

fn fibonacci(n: i64) -> i64
    return match n
        0 =>
            0
        end

        1 =>
            1
        end

        n =>
            fibonacci(n - 1) + fibonacci(n - 2)
        end
    end
end
